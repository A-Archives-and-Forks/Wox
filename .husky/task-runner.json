{
   "tasks": [
     {
       "name": "dotnet-format",
       "group": "pre-commit",
       "command": "dotnet",
       "args": ["format", "--include", "${staged}"],
       "include": ["**/*.cs"]
     },
     {
       "name": "warning-check",
       "command": "dotnet",
       "group": "pre-commit",
       "args": ["build", "/warnaserror"],
       "include": ["**/*.cs"]
     }
   ]
}
